@using scheapp.app.Models.Data.TableModels.Customers
@model List<Customer>
@{
    Layout = "_BusinessLayout";
    ViewData["Title"] = "Business Customers";
}
<h4>Customers</h4>
<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cust in Model)
        {
            <tr>
                <td>
                    @cust.FirstName
                </td>
                <td>
                    @cust.MiddleName
                </td>
                <td>
                    @cust.LastName
                </td>
            </tr>
        }
    </tbody>
</table>